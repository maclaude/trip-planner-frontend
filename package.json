{
  "name": "trip-planner-frontend",
  "description": "Frontend repository of the Trip-planner project",
  "author": "Marc-Antoine CLAUDE <claude.marcantoine2@gmail.com>",
  "license": "MIT",
  "version": "1.0.0",
  "engines": {
    "node": "^10.16.0",
    "yarn": "^1.17.0"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules",
    "build:dev": "NODE_ENV=development webpack",
    "build:dev:win": "set NODE_ENV=development && webpack",
    "build:prod": "node -p 'process.env' && NODE_ENV=production webpack --mode production",
    "build:prod:win": "set NODE_ENV=production && webpack --mode production",
    "save-deps": "rm -rf node_modules && npm update --save && npm update --save-dev",
    "lint": "eslint --fix 'src/**/*.js'"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/plugin-proposal-object-rest-spread": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "autoprefixer": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "css-loader": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-import-resolver-webpack": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "1.7.0",
    "file-loader": "*",
    "html-webpack-plugin": "*",
    "ignore-styles": "*",
    "mini-css-extract-plugin": "*",
    "node-sass": "*",
    "optimize-css-assets-webpack-plugin": "*",
    "postcss": "*",
    "postcss-loader": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "stylelint": "*",
    "stylelint-config-recommended": "*",
    "terser-webpack-plugin": "^1.3.0",
    "uglifyjs-webpack-plugin": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@blueprintjs/core": "^3.14.1",
    "@blueprintjs/datetime": "^3.7.2",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "dateformat": "^3.0.3",
    "dotenv-webpack": "^1.7.0",
    "google-map-react": "^1.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-css-loaders": "^0.0.5",
    "react-dom": "^16.8.4",
    "react-geocode": "^0.1.2",
    "react-icons": "^3.5.0",
    "react-redux": "^6.0.1",
